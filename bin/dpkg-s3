#!/usr/bin/env ruby
# frozen_string_literal: true

require 'pathname'
$LOAD_PATH.unshift File.join(Pathname.new(__FILE__).realpath, '../../lib')

require 'rubygems'
require 'dpkg/s3/cli'

begin
  Dpkg::S3::CLI.start
rescue StandardError
  warn "\n\tError during processing: #{$ERROR_INFO.message}\n\n"
end
